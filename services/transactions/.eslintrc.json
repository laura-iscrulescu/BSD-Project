{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "require-await": [
            "error"
        ],
        "no-tabs": "off",
        "indent": [
          2,
          "tab",
          {
            "SwitchCase": 1
          }
        ]
    }
}